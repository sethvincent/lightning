{
  "name": "lightning-server",
  "description": "lightning dataviz notebooks",
  "version": "1.0.1",
  "author": {
    "name": "Matthew Conlen",
    "email": "code@mathisonian.com"
  },
  "window": {
    "toolbar": true
  },
  "main": "electron/index.js",
  "keywords": [
    "lightning",
    "data visualization",
    "dataviz",
    "d3",
    "three.js",
    "data viz",
    "lightning-viz",
    "data server"
  ],
  "bin": {
    "lightning-server": "bin/lightning-server.js"
  },
  "engines": {
    "node": "0.10.x"
  },
  "homepage": "http://lightning-viz.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/lightning-viz/lightning"
  },
  "scripts": {
    "start": "NODE_ENV=development nodemon server.js --watch server.js ",
    "electron": "electron electron/",
    "test": "mocha --timeout 20000",
    "build": "gulp build",
    "watch": "gulp",
    "migrate": "./node_modules/.bin/sequelize db:migrate --config ./config/database.js",
    "createdb": "psql -c 'create database \"lightning-viz\";'",
    "postinstall": "npm run build && npm run fetch-visualizations",
    "electron-rebuild": "npm run rebuild-sqlite3; npm run rebuild-node-sass",
    "rebuild-sqlite3": "cd node_modules/sqlite3 && npm run prepublish && node-gyp configure --module_name=node_sqlite3 --module_path=../lib/binding/node-v44-darwin-x64 && node-gyp rebuild --target=0.30.0 --arch=x64 --target_platform=darwin --dist-url=https://atom.io/download/atom-shell --module_name=node_sqlite3 --module_path=../lib/binding/node-v44-darwin-x64",
    "rebuild-node-sass": "cd node_modules/node-sass && set HOME=~/.electron-gyp && node-gyp rebuild --target=0.30.0 --arch=x64 --target_platform=darwin --dist-url=https://atom.io/download/atom-shell --module_name=node_sass --module_path=../vendor/darwin-x64-44",
    "fetch-visualizations": "node ./tasks/get_default_visualizations.js",
    "generate-standalone-js": "gulp standalone-js"
  },
  "browserify": {
    "transform": [
      "reactify",
      "jadeify"
    ]
  },
  "license": "MIT",
  "dependencies": {
    "async": "^0.9.0",
    "basic-auth-connect": "^1.0.0",
    "body-parser": "^1.4.3",
    "browserify": "^11.0.1",
    "colors": "~0.6.2",
    "command-exists": "^0.1.0",
    "compression": "^1.0.8",
    "connect-slashes": "^1.2.0",
    "cookie-parser": "^1.3.2",
    "cookie-session": "^1.0.2",
    "cors": "^2.5.2",
    "debug": "^2.2.0",
    "easyimage": "^1.0.1",
    "express": "^4.13.1",
    "forever": "^0.11.1",
    "fs-extra": "^0.11.1",
    "gift": "^0.4.3-1",
    "glob": "^4.0.6",
    "gulp": "~3.8.0",
    "gulp-browserify": "~0.5.0",
    "gulp-browserify-protect": "^0.1.0",
    "gulp-csso": "~0.2.7",
    "gulp-if": "0.0.5",
    "gulp-livereload": "~1.3.1",
    "gulp-sass": "^2.1.0",
    "gulp-uglify": "~0.2.1",
    "gulp-util": "~2.2.14",
    "highlight.js": "^8.2.0",
    "immutable": "^3.7.4",
    "jade": "^1.11.0",
    "jadeify": "^4.4.0",
    "knox": "^0.9.0",
    "lodash": "^3.8.0",
    "marked": "~0.3.2",
    "method-override": "^2.1.1",
    "moment": "^2.10.6",
    "multiparty": "^3.3.1",
    "node-sass": "^3.4.2",
    "node-uuid": "^1.4.1",
    "npm": "^2.12.1",
    "path": "~0.4.9",
    "pg": "^4.4.3",
    "pg-hstore": "^2.3.2",
    "pym.js": "^0.4.1",
    "q": "~1.0.1",
    "randomstring": "^1.0.3",
    "react": "^0.13.3",
    "react-highlight": "^0.5.0",
    "react-radio-group": "^2.0.2",
    "react-simpletabs": "^0.6.1",
    "reactify": "^1.1.1",
    "request": "~2.36.0",
    "resumer": "0.0.0",
    "sequelize": "^3.14.1",
    "sequelize-cli": "^1.7.4",
    "serve-favicon": "^2.1.6",
    "serve-static": "^1.10.0",
    "socket.io": "^1.3.6",
    "sqlite3": "^3.1.1",
    "sticky-session": "^0.1.0",
    "superagent": "^0.18.2",
    "tiny-lr": "^0.1.6",
    "title-case": "^1.1.1",
    "validator": "~3.17.0",
    "view-helpers": "^0.1.5",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "webshot": "^0.15.3",
    "winston": "^0.7.3",
    "yargs": "^1.3.3"
  },
  "devDependencies": {
    "electron": "^0.4.1",
    "menubar": "^2.0.16",
    "nodemon": "*",
    "mocha": "^1.21.5",
    "expect.js": "^0.3.1"
  }
}
